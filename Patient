package HospitalManagementSystem;

public class Patient extends Person {

    private int age;

    public Patient(String id, String name, int age) {
        super(id, name);
        this.age = age;
    }

    public int getAge() {
        return age;
    }

    @Override
    public String getRole() {
        return "Patient";
    }

	public void setVisible1(boolean b) {
		
	}

	public void setVisible(boolean b) {
		
	}
}
