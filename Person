package HospitalManagementSystem;

public abstract class Person {
    private String id;
    private String name;

    public Person(String id, String name) {
        this.id = id;
        this.name = name;
    }

    // Encapsulation
    public String getId() { return id; }
    public String getName() { return name; }

    // Abstraction
    public abstract String getRole();
}
